{
	"dependencies": {
		"@material/theme": "^14.0.0",
		"@tauri-apps/api": "^2.3.0",
		"@tauri-apps/plugin-dialog": "^2.2.0",
		"@tauri-apps/plugin-opener": "^2.2.6",
		"@tauri-apps/plugin-os": "^2.2.0"
	},
	"description": "A cross platform media manager and player built on libVLC",
	"devDependencies": {
		"@mdi/js": "^7.4.47",
		"@smui/button": "8.0.0-beta.3",
		"@smui/card": "8.0.0-beta.3",
		"@smui/checkbox": "8.0.0-beta.3",
		"@smui/circular-progress": "8.0.0-beta.3",
		"@smui/common": "8.0.0-beta.3",
		"@smui/dialog": "8.0.0-beta.3",
		"@smui/drawer": "8.0.0-beta.3",
		"@smui/list": "8.0.0-beta.3",
		"@smui/menu": "8.0.0-beta.3",
		"@smui/radio": "8.0.0-beta.3",
		"@smui/select": "8.0.0-beta.3",
		"@smui/slider": "8.0.0-beta.3",
		"@smui/snackbar": "8.0.0-beta.3",
		"@smui/switch": "8.0.0-beta.3",
		"@smui/tab": "8.0.0-beta.3",
		"@smui/textfield": "8.0.0-beta.3",
		"@smui/tooltip": "8.0.0-beta.3",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.19.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tauri-apps/cli": "^2.3.1",
		"smui-theme": "8.0.0-beta.3",
		"svelte": "^5.22.6",
		"svelte-check": "^4.1.5",
		"typescript": "~5.8.2",
		"vite": "^6.2.1"
	},
	"license": "GPL-3.0-or-later",
	"name": "conte-curator",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"preview": "pnpm prepare && vite preview",
		"tauri": "tauri",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules"
	},
	"type": "module",
	"version": "1.0.0-alpha",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher"
		]
	}
}
